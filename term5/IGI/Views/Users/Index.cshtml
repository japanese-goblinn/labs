@model IEnumerable<User>

@{
    ViewData["Title"] = "Users";
}

<div class="text-left mt-3">
    <h3 class="display-6">@ViewData["Title"]</h3>
</div>
<hr/>
<div>
    @foreach (var user in Model)
    {
        <div class="container"> 
            <tr>
                <td>
                   @Html.DisplayFor(users => user.Name)
                </td>
                <td>
                   @Html.DisplayFor(users => user.Username)
                </td>
            </tr>
        </div>
        
    }   
</div>

